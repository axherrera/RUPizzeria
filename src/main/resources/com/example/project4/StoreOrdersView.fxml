      <Button layoutX="162.0" layoutY="357.0" mnemonicParsing="false" onAction="#cancelOrder" text="Cancel Order" />
      <Button layoutX="348.0" layoutY="357.0" mnemonicParsing="false" onAction="#exportOrder" text="Export Orders" />
      <Label layoutX="32.0" layoutY="83.0" text="Order Number :" />
      <Label layoutX="332.0" layoutY="83.0" text="Order Total :" />
      <TextField fx:id="orderTotal" layoutX="410.0" layoutY="78.0" />
      <ComboBox fx:id="orderNum" layoutX="124.0" layoutY="79.0" onAction="#chooseOrder" prefWidth="150.0" />
   </children>
</AnchorPane>
